  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Sheas Core">
      <HintPath>C:\Users\Space Time\Desktop\Sheas Core\bin\Release\Sheas Core.dll</HintPath>
    </Reference>
  </ItemGroup>

